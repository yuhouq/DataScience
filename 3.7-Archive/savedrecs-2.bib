@article{ ISI:000174387400002,
Author = {Baskerville, RL and Myers, MD},
Title = {{Information systems as a reference discipline}},
Journal = {{MIS QUARTERLY}},
Year = {{2002}},
Volume = {{26}},
Number = {{1}},
Pages = {{1-14}},
Month = {{MAR}},
Abstract = {{The conventional wisdom amongst information systems (IS) researchers is
   that information systems is an applied discipline drawing upon other,
   more fundamental, reference disciplines. These reference disciplines are
   seen as having foundational value for IS. We believe that it is time to
   question the conventional wisdom. We agree that many disciplines are
   relevant for IS researchers, but we suggest a re-think of the idea of
   ``reference disciplines{''} for IS. In a sense, IS has come of age.
   Perhaps the time has come for IS to become a reference discipline for
   others.}},
