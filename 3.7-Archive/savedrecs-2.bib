@article{ ISI:000173923600002,
Author = {Cooper, BL and Watson, HJ and Wixom, BH and Goodhue, DL},
Title = {{Data warehousing supports corporate strategy at First American
   Corporation}},
Journal = {{MIS QUARTERLY}},
Year = {{2000}},
Volume = {{24}},
Number = {{4}},
Pages = {{547-567}},
Month = {{DEC}},
Abstract = {{From 1990 through 1998, First American Corporation (FA C) changed its
   corporate strategy from a traditional banking approach to a customer
   relationship-oriented strategy that placed FAC's customers at the center
   of all aspects of the company's operations. The transformation made FAC
   an innovative leader in the financial services industry. This case study
   describes FAC's transformation and the way in which a data warehouse
   called VISION helped make it happen. FAC's experiences suggest lessons
   for managers who plan to use technology to support changes that are
   designed to significantly improve organizational performance. In
   addition, they raise interesting questions about the means by which
   information technology can be used to gain competitive advantage.}},
