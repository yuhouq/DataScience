@article{ ISI:000187244200003,
Author = {Sharma, R and Yetton, P},
Title = {{The contingent effects of management support and task interdependence on
   successful information systems implementation}},
Journal = {{MIS QUARTERLY}},
Year = {{2003}},
Volume = {{27}},
Number = {{4}},
Pages = {{533-555}},
Month = {{DEC}},
Abstract = {{Management support is considered to be a critical factor in the
   successful implementation of information systems innovations. The
   literature suggests a complex relationship between management support
   and implementation success. However, the empirical literature typically
   hypothesizes and tests a simple main-effects model. Drawing upon the
   role of the institutional context and metastructuration actions, we
   propose a contingent model in which task interdependence moderates the
   effect of management support on implementation success. A meta-analysis
   of the empirical literature provides strong support for the model and
   begins to explain the wide variance in empirical findings. Implications
   for theory and practice are discussed.}},
