@article{ ISI:000183386200002,
Author = {Benbasat, I and Zmud, RW},
Title = {{The identity crisis within the is discipline: Defining and communicating
   the discipline's core properties}},
Journal = {{MIS QUARTERLY}},
Year = {{2003}},
Volume = {{27}},
Number = {{2}},
Pages = {{183-194}},
Month = {{JUN}},
Abstract = {{We are concerned that the IS research community is making the
   discipline's central identity ambiguous by, all too frequently,
   under-investigating phenomena intimately associated with IT-based
   systems and over-investigating phenomena distantly associated with
   IT-based systems. In this commentary, we begin by discussing why
   establishing an identity for the IS field is important. We then describe
   what such an identity may look like by proposing a core set of
   properties, i.e., concepts and phenomena, that define the IS field.
   Next, we discuss research by IS scholars that either fails to address
   this core set of properties (labeled as error of exclusion) or that
   addresses concepts/phenomena falling outside this core set (labeled as
   error of inclusion). We conclude by offering suggestions for redirecting
   IS scholarship toward the concepts and phenomena that we argue define
   the core of the IS discipline.}},
